{
  "name": "tilegen",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run index.ts",
    "tool:filter-geojson": "bun run tools/filter-geojson.ts",
    "tool:generate-landcover-tiles": "bun run tools/generate-landcover-tiles.ts",
    "tool:generate-base-tiles": "bun run tools/generate-base-tiles.ts",
    "tool:generate-games": "POLYGON_CLIPPING_MAX_QUEUE_SIZE=1000000000 POLYGON_CLIPPING_MAX_SWEEPLINE_SEGMENTS=1000000000 bun run tools/generate-games.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@types/geojson": "^7946.0.16",
    "happy-dom": "^18.0.1",
    "path-data-polyfill": "^1.0.10",
    "polygon-clipping": "^0.15.7",
    "svg-path-properties": "^2.0.0"
  }
}
